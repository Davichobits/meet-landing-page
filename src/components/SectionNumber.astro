---
interface Props {
  number: number;
}
const {number} = Astro.props;
---

<div class='flex flex-col items-center'>
  <div class='h-20 w-0 border border-transparent border-l-Grey'></div>
  <div
    class='size-14 border rounded-full border-Grey grid place-content-center'
  >
    {number}
  </div>
</div>
